{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],

  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],

    
    "pdf": {
      "content": [
        {
          "files": [
            "api/**.yml",
            "api-vb/**.yml"
          ],
          "exclude": [
            "**/toc.yml",
            "**/toc.md"
          ]
        },
        {
          "files": [
            "articles/**.md",
            "articles/**/toc.yml",
            "toc.yml",
            "*.md",
            "pdf/*"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site_pdf/**",
            "**/toc.yml",
            "**/toc.md"
          ]
        },
        {
          "files": "pdf/toc.yml"
        }
      ],
      "resource": [
        {
          "files": [
            "images/**"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site_pdf/**"
          ]
        }
      ],
      "overwrite": [
        {
          "files": [
            "apidoc/**.md"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site_pdf/**"
          ]
        }
      ],
      "dest": "_site_pdf"
    },


    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}